{
  "name": "@gbgplc/smartcapture-web",
  "description": "Docs & Bio Web Components",
  "license": "SEE LICENSE IN EULA.txt",
  "author": "GBG Plc",
  "version" :"1.1.7",
  "type": "module",
  "main": "dist/lib/index.js",
  "module": "dist/lib/index.es.js",
  "types": "dist/lib/index.d.ts",
  "exports": {
    ".": "./dist/lib/index.es.js",
    "./face": "./dist/lib/face/index.js",
    "./document": "./dist/lib/document/index.js",
    "./image": "./dist/lib/image/index.js"
  },
  "files": [
    "dist/lib",
    "dist/assets",
    "dist/custom-elements.json",
    "EULA.txt"
  ],
  "engines": {
    "node": ">=20"
  },
  "packageManager": "yarn@1.22.19",
  "scripts": {
    "analyze": "cem analyze --config custom-elements-manifest.config.js",
    "prepare": "yarn --cwd demo install && (cd demo/node_modules/$npm_package_name && rimraf demo node_modules dist && mkdir dist) && ln -sf \"$PWD\"/dist/* demo/node_modules/$npm_package_name/dist && husky install",
    "dev": "yarn build && concurrently -k -r \"wds\" \"rollup -c rollup.config.js --watch\"",
    "start": "concurrently -k -r \"wds\" \"rollup -c rollup.config.js --watch\"",
    "start-only": "wds",
    "build": "rimraf dist && tsc && rollup -c rollup.config.js && yarn analyze",
    "prepublish": "yarn build",
    "lint": "eslint --ext .ts,.html . --ignore-path .gitignore && prettier \"**/*.ts\" --check --ignore-path .gitignore",
    "lint-for-ci": "eslint --ext .ts,.html . --ignore-path .gitignore -f json -o eslint-out.json",
    "format": "eslint --ext .ts,.html . --fix --ignore-path .gitignore && prettier \"**/*.ts\" --write --ignore-path .gitignore",
    "test": "rimraf dist && tsc && rollup -c rollup.config.js && wtr --coverage",
    "generate-docs": "cd docs && bundle exec jekyll serve -d \"../demo/sdk-docs\"",
    "test-only": "wtr --coverage",
    "test:watch": "tsc && concurrently -k -r \"tsc --watch --preserveWatchOutput\" \"wtr --watch\"",
    "co:login": "aws codeartifact login --tool npm --repository smartcapture-web --domain gbgplc --domain-owner 266130098155 --region eu-west-2 --namespace @gbgplc"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "@gbgplc/stickman-wasm": "^1.81.0",
    "@lottiefiles/lottie-player": "^2.0.2",
    "idlive-face-capture-web": "file:idlive-face-capture-web-production-2.1.1.tgz",
    "idlive-face-capture-web-development": "file:./idlive-face-capture-web-development-2.1.1.tgz",
    "lit": "^3.1.3",
    "piexifjs": "^1.0.6",
    "tslib": "^2.3.1"
  },
  "bundledDependencies": [
    "@gbgplc/stickman-wasm",
    "idlive-face-capture-web",
    "idlive-face-capture-web-development"
  ],
  "devDependencies": {
    "@babel/core": "^7.0.0-0",
    "@custom-elements-manifest/analyzer": "^0.10.3",
    "@open-wc/testing": "^4.0.0",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-replace": "^6.0.1",
    "@rollup/plugin-terser": "^0.4.3",
    "@rollup/plugin-typescript": "^12.1.1",
    "@types/mocha": "^10.0.9",
    "@typescript-eslint/eslint-plugin": "^8.13.0",
    "@typescript-eslint/parser": "^8.13.0",
    "@web/dev-server": "^0.4.6",
    "@web/dev-server-rollup": "^0.6.4",
    "@web/test-runner": "^0.19.0",
    "@web/test-runner-puppeteer": "^0.17.0",
    "caniuse-lite": "^1.0.30001625",
    "concurrently": "^9.1.0",
    "eslint": "^9.14.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin": "^1.0.1",
    "eslint-plugin-html": "^8.1.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-lit": "^1.8.3",
    "eslint-plugin-lit-a11y": "^4.1.1",
    "eslint-plugin-no-only-tests": "^3.1.0",
    "eslint-plugin-wc": "^2.2.0",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "rollup": "^4.24.4",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-dts": "^6.1.1",
    "sinon": "^19.0.2",
    "sonar-scanner": "^3.1.0",
    "typescript": "^5.6.3"
  },
  "customElements": "dist/custom-elements.json",
  "prettier": {
    "singleQuote": true,
    "arrowParens": "avoid"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
