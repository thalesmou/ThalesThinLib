{
  "type": "module",
  "devDependencies": {
    "@web/test-runner": "^0.18.0",
    "@web/test-runner-commands": "^0.9.0",
    "@web/test-runner-puppeteer": "^0.15.0",
    "base64-arraybuffer": "^1.0.2",
    "puppeteer": "^22.3.0",
    "sha1-uint8array": "^0.10.7"
  },
  "name": "@gbgplc/stickman-wasm",
  "description": "Wasm wrapper for smart-capture (idscan-engine-smart-capture)",
  "version" :"1.81.0",
  "main": "_builds/stickman.js",
  "files": [
    "_builds/stickman.*"
  ],
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build": "npm run em.config && npm run em.build",
    "test": "wtr --config web-test-runner.config.mjs",
    "em.config": "emcmake cmake -H. -B_builds -G\"Unix Makefiles\" -DCMAKE_TOOLCHAIN_FILE=${POLLY_ROOT}/emscripten-bulkm-wasm-exceptions-cxx17.cmake -DCMAKE_C_ABI_COMPILED=ON -DCMAKE_CXX_ABI_COMPILED=ON -DCMAKE_CROSSCOMPILING_EMULATOR=${CMAKE_CROSSCOMPILING_EMULATOR:-node} -DLIBRARY_VERSION=${LIBRARY_VERSION}",
    "em.build": "cmake --build _builds --config Release",
    "em.test": "cd _builds && ctest -C Release -V",
    "co:login": "aws codeartifact login --tool npm --repository smartcapture-web --domain gbgplc --domain-owner 266130098155 --region eu-west-2 --namespace @gbgplc"
  },
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "https://github.com/gbgplc-internal/docsbio-stickman-wasm"
  }
}
